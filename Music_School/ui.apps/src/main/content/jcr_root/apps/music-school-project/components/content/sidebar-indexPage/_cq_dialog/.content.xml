<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Tabs"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_list_v1">
    <content
        granite:class="core-wcm-list-v1"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 1"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <card-title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Card Title 1"
                                        name="./cardTitle1"
                                        required="{Boolean}true"/>
                                    <card-link1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Card Link 1"
                                        name="./cardLink1"/>
                                    <file1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset 1"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference1"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <date-select1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Select Date - 1"
                                        name="./date1"
                                        required="{Boolean}true"/>
                                    <description-1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Card 1 Description"
                                        name="./cardDiscription1"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </tab1>
                    <tab2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 2"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <card-title2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Card Title 2"
                                        name="./cardTitle2"
                                        required="{Boolean}true"/>
                                    <date-select2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Select Date - 2"
                                        name="./date2"
                                        required="{Boolean}true"/>
                                    <description-2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Card 2 Description"
                                        name="./cardDiscription2"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </tab2>
                    <tab3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 3"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <card-title3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Card Title 3"
                                        name="./cardTitle3"
                                        required="{Boolean}true"/>
                                    <card-link3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Card Link 3"
                                        name="./cardLink3"/>
                                    <file3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset 1"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference3"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <date-select3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Select Date - 3"
                                        name="./date3"
                                        required="{Boolean}true"/>
                                    <description-3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Card 3 Description"
                                        name="./cardDiscription3"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </tab3>
                    <tab4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 4"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <card-title4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Card Title 4"
                                        name="./cardTitle4"
                                        required="{Boolean}true"/>
                                    <card-link4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Card Link 4"
                                        name="./cardLink4"/>
                                    <file4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset 1"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference4"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <date-select4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Select Date - 4"
                                        name="./date4"
                                        required="{Boolean}true"/>
                                    <description-4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Card 4 Description"
                                        name="./cardDiscription4"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </tab4>
                    <tab5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 5"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <card-title5
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Card Title 5"
                                        name="./cardTitle5"
                                        required="{Boolean}true"/>
                                    <date-select5
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Select Date - 5"
                                        name="./date5"
                                        required="{Boolean}true"/>
                                    <description-5
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Card 5 Description"
                                        name="./cardDiscription5"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </tab5>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
