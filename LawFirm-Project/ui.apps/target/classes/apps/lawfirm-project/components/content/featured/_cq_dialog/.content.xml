<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Featured"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.list.v1.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_list_v1">
    <content
        granite:class="cmp-list--editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <title-tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Title Here"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Title Here"
                                        name="./title"/>
                                </items>
                            </column>
                        </items>
                    </title-tab>
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 1"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="First Title"
                                        name="./title1"/>
                                    <titletext1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Title Text"
                                        name="./titletext1"/>
                                    <linktext1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Link Text Here"
                                        name="./text1"/>
                                    <link1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Second Half Text Here"
                                        name="./link1"/>
                                    <file1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference1"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <event1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Event #1"
                                        name="./date1"/>
                                </items>
                            </column>
                        </items>
                    </tab1>
                    <tab2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 2"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="First Title"
                                        name="./title2"/>
                                    <titletext2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Title Text"
                                        name="./titletext2"/>
                                    <linktext1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Link Text Here"
                                        name="./text2"/>
                                    <link2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Second Half Text Here"
                                        name="./link2"/>
                                    <file2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference2"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <event2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Event #2"
                                        name="./date2"/>
                                </items>
                            </column>
                        </items>
                    </tab2>
                    <tab3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 3"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="First Title"
                                        name="./title3"/>
                                    <titletext3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Title Text"
                                        name="./titletext3"/>
                                    <linktext3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Link Text Here"
                                        name="./text3"/>
                                    <link3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Second Half Text Here"
                                        name="./link3"/>
                                    <file3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference3"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <event3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Event #3"
                                        name="./date3"/>
                                </items>
                            </column>
                        </items>
                    </tab3>
                    <tab4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 4"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="First Title"
                                        name="./title4"/>
                                    <titletext4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Title Text"
                                        name="./titletext4"/>
                                    <linktext4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Link Text Here"
                                        name="./text4"/>
                                    <link4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Leave empty to use the page title."
                                        fieldLabel="Second Half Text Here"
                                        name="./link4"/>
                                    <file4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image asset"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./fileReference4"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <event4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Event #4"
                                        name="./date4"/>
                                </items>
                            </column>
                        </items>
                    </tab4>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
